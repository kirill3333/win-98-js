{
  "extends": [
    "plugin:react/recommended"
  ],
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "all",
        "useTabs": false
      }
    ]
  },

  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6
  }
}